@model MenuViewModel
@{
    ViewBag.Title = "Menu";
}

<h1 align="center">Pizza Menu</h1>
@for (int i = 0; i < Model.Menu.Count; i++)
{
    @Html.Partial("_PizzaItem", new PizzaViewModel()
    {
        Name = Model.Menu[i].Name,
        Price = Model.Menu[i].Price,
        Image = Model.Menu[i].Image
    })
}

